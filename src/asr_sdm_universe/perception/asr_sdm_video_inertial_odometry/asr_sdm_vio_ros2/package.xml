<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>asr_sdm_vio_ros2</name>
  <version>0.1.0</version>
  <description>ROS2 VIO frontend resources and demo nodes (ament).</description>
  <maintainer email="azxiyuanqian@outlook.com">lxy</maintainer>
  <license>Apache-2.0</license>

  <!-- Build tool -->
  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- Build dependencies -->
  <build_depend>rclcpp</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>tf2_ros</build_depend>
  <build_depend>cv_bridge</build_depend>
  <build_depend>ament_index_cpp</build_depend>
  <build_depend>video_kit_cameras</build_depend>
  <build_depend>message_filters</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>asr_sdm_vio</build_depend>

  <!-- Exported build dependencies (for downstream packages linking headers, none for now) -->
  <build_export_depend>rclcpp</build_export_depend>
  <build_export_depend>sensor_msgs</build_export_depend>
  <build_export_depend>geometry_msgs</build_export_depend>
  <build_export_depend>tf2_ros</build_export_depend>
  <build_export_depend>cv_bridge</build_export_depend>
  <build_export_depend>ament_index_cpp</build_export_depend>
  <build_export_depend>video_kit_cameras</build_export_depend>
  <build_export_depend>asr_sdm_vio</build_export_depend>
  <build_export_depend>message_filters</build_export_depend>
  <build_export_depend>nav_msgs</build_export_depend>

  <!-- Runtime dependencies -->
  <exec_depend>rclcpp</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>tf2_ros</exec_depend>
  <exec_depend>cv_bridge</exec_depend>
  <exec_depend>ament_index_cpp</exec_depend>
  <exec_depend>video_kit_cameras</exec_depend>
  <exec_depend>message_filters</exec_depend>
  <exec_depend>nav_msgs</exec_depend>
  <exec_depend>asr_sdm_vio</exec_depend>
  <depend>asr_sdm_vio_ceres_backend</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
